{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "product"
    },
    "amount": {
      "type": "integer",
      "default": 1
    }
  }
}
